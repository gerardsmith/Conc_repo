<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="15dp">


    <TextView
        android:text="@string/hia1_form"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView38"
        android:textAppearance="?android:attr/textAppearanceLarge"/>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:id="@+id/linearLayout001"
        android:weightSum="1"
        android:layout_below="@+id/textView38"
        android:paddingTop="50dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="@string/hia1a_section71_header"
            android:id="@+id/labelTextView001"
            android:textSize="16sp"
            android:layout_weight="1.05" />

        <Spinner
            android:layout_width="203dp"
            android:layout_height="wrap_content"
            android:id="@+id/spinner" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout002"
        android:weightSum="1"
        android:layout_below="@+id/linearLayout001"
        android:paddingTop="20dp">


        <TextView
            android:text="@string/vid_influence"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:id="@+id/textView002"
            android:textSize="16sp"
            android:layout_weight="1" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout003"
        android:weightSum="1"
        android:layout_below="@+id/linearLayout002"
        android:paddingTop="15dp">

        <CheckBox
            android:text="@string/yes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/checkBox_VI_Y"
            android:layout_weight="0.14" />

        <CheckBox
            android:text="@string/no"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/checkBox_VI_N"
            android:layout_weight="1" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout004"
        android:layout_below="@+id/linearLayout003"
        android:weightSum="1"
        android:paddingTop="25dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="@string/hia1a_section81_header"
            android:id="@+id/labelTextView003"
            android:textSize="16sp"
            android:layout_weight="1.05" />

        <Spinner
            android:layout_width="203dp"
            android:layout_height="wrap_content"
            android:id="@+id/spinner2" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout005"
        android:weightSum="1"
        android:layout_below="@+id/linearLayout004"
        android:paddingTop="20dp">


        <TextView
            android:text="@string/MDD_present"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:id="@+id/textView003"
            android:textSize="16sp"
            android:layout_weight="1" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout006"
        android:weightSum="1"
        android:layout_below="@+id/linearLayout005"
        android:paddingTop="15dp">

        <CheckBox
            android:text="@string/yes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/checkBox_MDD_Y"
            android:layout_weight="0.14" />

        <CheckBox
            android:text="@string/no"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/checkBox_MDD_N"
            android:layout_weight="1" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout007"
        android:layout_below="@+id/linearLayout006"
        android:weightSum="1"
        android:paddingTop="25dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="@string/hia1a_section91_header"
            android:id="@+id/labelTextView004"
            android:textSize="16sp"
            android:layout_weight="1.05" />

        <Spinner
            android:layout_width="203dp"
            android:layout_height="wrap_content"
            android:id="@+id/spinner3" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout008"
        android:weightSum="1"
        android:layout_below="@+id/linearLayout007"
        android:paddingTop="20dp">


        <TextView
            android:text="@string/vid_influence2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:id="@+id/textView004"
            android:textSize="16sp"
            android:layout_weight="1" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout009"
        android:weightSum="1"
        android:layout_below="@+id/linearLayout008"
        android:paddingTop="15dp">

        <CheckBox
            android:text="@string/yes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/checkBox_VI2_Y"
            android:layout_weight="0.14" />

        <CheckBox
            android:text="@string/no"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/checkBox_VI2_N"
            android:layout_weight="1" />
    </LinearLayout>

</RelativeLayout>